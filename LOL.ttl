LoL:ability_description rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain LoL:Abilità ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty LoL:scales_with_stat ;
                                      owl:someValuesFrom LoL:Statistiche
                                    ] ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Descrizione dell'abilità"@it ;
                        rdfs:label "ability_description"@it .


###  http://www.semanticweb.org/drunkfazor/ontologies/2024/2/LoL#has_price
LoL:has_price rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain LoL:Entity ,
                          LoL:Oggetto ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty LoL:has_stat ;
                            owl:someValuesFrom LoL:Statistiche
                          ] ;
              rdfs:range xsd:int ;
              rdfs:comment "Prezzo dell'oggetto"@it ;
              rdfs:label "has_price"@it .


###  http://www.semanticweb.org/drunkfazor/ontologies/2024/2/LoL#lore
LoL:lore rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain LoL:Campione ,
                     LoL:Entity ,
                     wd:Q1062345 ,
                     [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                              owl:onProperty LoL:has_ability ;
                                              owl:someValuesFrom LoL:Passiva
                                            ]
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty LoL:has_ability ;
                                              owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                              owl:onClass LoL:Abilità
                                            ]
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty LoL:has_ability ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass LoL:Ultimate
                                            ]
                                          ) ;
                       rdf:type owl:Class
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty LoL:has_ability ;
                       owl:someValuesFrom LoL:Passiva
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :part_of_class ;
                       owl:someValuesFrom LoL:Classe
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty LoL:has_ability ;
                       owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                       owl:onClass LoL:Abilità
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty LoL:has_ability ;
                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                       owl:onClass LoL:Ultimate
                     ] ;
         rdfs:range xsd:string ;
         rdfs:comment "Storia del campione"@it ;
         rdfs:label "lore"@it .


###  http://www.semanticweb.org/drunkfazor/ontologies/2024/2/LoL#match_date
LoL:match_date rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain LoL:Match ,
                           LoL:participation:Event ,
                           <https://www.bbc.co.uk/ontologies/sport-ontology/#Match> ,
                           wd:Q12362855 ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty LoL:has_modality ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass LoL:Modalità
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty LoL:played_on_map ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass LoL:Mappa
                           ] ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Data del match"@it ;
               rdfs:label "match_date"@it .


###  http://www.semanticweb.org/drunkfazor/ontologies/2024/2/LoL#number_player
LoL:number_player rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain LoL:Modalità ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty LoL:played_in_map ;
                                owl:someValuesFrom LoL:Mappa
                              ] ;
                  rdfs:range xsd:int ;
                  rdfs:comment "Numero di giocatori"@it ;
                  rdfs:label "number_player"@it .


###  http://www.semanticweb.org/drunkfazor/ontologies/2024/2/LoL#rank
LoL:rank rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain <http://data.totl.net/game.rdf#Player> ,
                     LoL:Player ,
                     LoL:participation:Object ,
                     wd:Q5276395 ;
         rdfs:range xsd:string ;
         rdfs:comment "rank del giocatore"@it ;
         rdfs:label "rank"@it .


###  http://www.semanticweb.org/drunkfazor/ontologies/2024/2/LoL#release_date
LoL:release_date rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain LoL:Campione ,
                             LoL:Entity ,
                             wd:Q1062345 ,
                             [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty LoL:has_ability ;
                                                      owl:someValuesFrom LoL:Passiva
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty LoL:has_ability ;
                                                      owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                                      owl:onClass LoL:Abilità
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty LoL:has_ability ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass LoL:Ultimate
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty LoL:has_ability ;
                               owl:someValuesFrom LoL:Passiva
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :part_of_class ;
                               owl:someValuesFrom LoL:Classe
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty LoL:has_ability ;
                               owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                               owl:onClass LoL:Abilità
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty LoL:has_ability ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass LoL:Ultimate
                             ] ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "Data di rilascio"@it ;
                 rdfs:label "release_date"@it .


