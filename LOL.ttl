[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Asserisce che se un giocatore ha giocato un campione in un ruolo, allora il campione è giocato in quel ruolo" ;
   rdfs:label "giocatoInRuolo" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate LOL:Match_role ;
                           swrl:argument1 <http://www.semanticweb.org/gfazor/ontologies/LOL/x>
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:ClassAtom ;
                                      swrl:classPredicate LOL:Player ;
                                      swrl:argument1 <http://www.semanticweb.org/gfazor/ontologies/LOL/y>
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate LOL:Loadout ;
                                                 swrl:argument1 <http://www.semanticweb.org/gfazor/ontologies/LOL/z>
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:ClassAtom ;
                                                            swrl:classPredicate LOL:Ruoli ;
                                                            swrl:argument1 <http://www.semanticweb.org/gfazor/ontologies/LOL/k>
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:ClassAtom ;
                                                                       swrl:classPredicate LOL:Campione ;
                                                                       swrl:argument1 <http://www.semanticweb.org/gfazor/ontologies/LOL/h>
                                                                     ] ;
