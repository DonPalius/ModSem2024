#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#vann:preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#vann:preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/gfazor/ontologies/LOL#ability_of
LOL:ability_of rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf LOL:isPartOf ;
               owl:inverseOf LOL:has_ability ;
               rdfs:domain LOL:Abilità ;
               rdfs:range LOL:Campione ;
               rdfs:comment "Abilità Usabile da un campione"@it ;
               rdfs:label "ability_of"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#belongs_to_class
LOL:belongs_to_class rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf LOL:hasPart ;
                     rdfs:domain LOL:Classe ;
                     rdfs:range LOL:Campione ;
                     rdfs:comment "Champions belonging to a class"@en ;
                     rdfs:label "belongs to class"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#bought_objects
LOL:bought_objects rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:comment "Relazione che indica di aver acquistato un oggetto"@it ;
                   rdfs:label "bought_objects"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#bought_stat
LOL:bought_stat rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain LOL:Loadout ;
                rdfs:range LOL:Statistiche ;
                owl:propertyChainAxiom ( LOL:bought_objects
                                         LOL:has_stat
                                       ) ;
                rdfs:comment "Relazione che indica di aver acquistato delle statistiche"@it ;
                rdfs:label "bought_stat"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#contains_keystone
LOL:contains_keystone rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf LOL:hasPart ;
                      owl:inverseOf LOL:keystone_in_path ;
                      rdfs:domain LOL:Path ;
                      rdfs:range LOL:Keystone ;
                      rdfs:comment "Relazione che indica che la keystone appartinene al path"@it ;
                      rdfs:label "contains_keystone"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#contains_rune
LOL:contains_rune rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf LOL:hasPart ;
                  owl:inverseOf LOL:in_path ;
                  rdfs:domain LOL:Path ;
                  rdfs:range LOL:Path_Rune ;
                  rdfs:comment "Relazione che indica che il path contiene quella runa"@it ;
                  rdfs:label "contains_rune"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#hasPart
LOL:hasPart rdf:type owl:ObjectProperty ;
            owl:inverseOf LOL:isPartOf .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_ability
LOL:has_ability rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf LOL:hasPart ;
                rdfs:domain LOL:Campione ;
                rdfs:range LOL:Abilità ;
                rdfs:comment "Relazione che indica che un campone possiede un abilità"@it ;
                rdfs:label "has_ability"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_keystone
LOL:has_keystone rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf LOL:hasPart ;
                 owl:inverseOf LOL:is_keystone_of ;
                 rdfs:domain LOL:Rune_Page ;
                 rdfs:range LOL:Keystone ;
                 rdfs:comment "Relazione che indica che una runa ha quella keystone"@it ;
                 rdfs:label "has_keystone"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_modality
LOL:has_modality rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain LOL:Match ;
                 rdfs:range LOL:Modalità ;
                 rdfs:comment "Relazione che indica che il match appartinene a un tipo di modalità"@it ;
                 rdfs:label "has_modality"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_primary_path
LOL:has_primary_path rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:domain LOL:Rune_Page ;
                     rdfs:range LOL:Path ;
                     rdfs:comment "Relazione che indica che una rune page appartinene a un tipo di primary path"@it ;
                     rdfs:label "has_primary_path"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_rune
LOL:has_rune rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf LOL:hasPart ;
             owl:inverseOf LOL:is_rune_of ;
             rdfs:domain LOL:Rune_Page ;
             rdfs:range LOL:Path_Rune ;
             rdfs:comment "Relazione che indica che una rune appartinene a un tipo di primary path"@it ;
             rdfs:label "has_primary_path"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_secondary_path
LOL:has_secondary_path rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain LOL:Rune_Page ;
                       rdfs:range LOL:Path ;
                       rdfs:comment "Relazione che indica che una rune appartinene a un tipo di secondary path"@it ;
                       rdfs:label "has_primary_path"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_shard
LOL:has_shard rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf LOL:hasPart ;
              owl:inverseOf LOL:is_shard_of ;
              rdfs:domain LOL:Rune_Page ;
              rdfs:range LOL:Shard ;
              rdfs:comment "Relazione che indica una rune page possiede una shard"@it ;
              rdfs:label "has_shard"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_stat
LOL:has_stat rdf:type owl:ObjectProperty ;
             rdfs:domain LOL:Entity ;
             rdfs:range LOL:Statistiche ;
             rdfs:comment "Relazione che un oggetto possiede una statistica"@it ;
             rdfs:label "has_stat"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#in_path
LOL:in_path rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf LOL:isPartOf ;
            rdfs:domain LOL:Path_Rune ;
            rdfs:range LOL:Path ;
            rdfs:comment "Relazione che indica che una keystone è presente in un path"@it ;
            rdfs:label "in_path"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#isPartOf
LOL:isPartOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#is_keystone_of
LOL:is_keystone_of rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf LOL:isPartOf ;
                   rdfs:domain LOL:Keystone ;
                   rdfs:range LOL:Rune_Page ;
                   rdfs:comment "Relazione che indica che la keystone appartinene al runa"@it ;
                   rdfs:label "is_keystone_of"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#is_rune_of
LOL:is_rune_of rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf LOL:isPartOf ;
               rdfs:domain LOL:Path_Rune ;
               rdfs:range LOL:Rune_Page ;
               rdfs:comment "Relazione che indica una runa appartinene a un path di rune"@it ;
               rdfs:label "is_rune_of"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#is_shard_of
LOL:is_shard_of rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf LOL:isPartOf ;
                rdfs:domain LOL:Shard ;
                rdfs:range LOL:Rune_Page ;
                rdfs:comment "Relazione che indica una shard appartinene a rune page"@it ;
                rdfs:label "is_rune_of"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#keystone_in_path
LOL:keystone_in_path rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf LOL:isPartOf ;
                     rdfs:domain LOL:Keystone ;
                     rdfs:range LOL:Path ;
                     rdfs:comment "elazione che indica che un path possiede una keystone"@it ;
                     rdfs:label "keystone_in_path"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#loadoutOfParticipant
LOL:loadoutOfParticipant rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf LOL:roleOfParticipant ;
                         rdfs:domain LOL:Match_role ;
                         rdfs:range LOL:Loadout ;
                         rdfs:comment "The loadout choosen by the player in a game"@en ;
                         rdfs:label "loadoutOfParticipant"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#objectParticipating
LOL:objectParticipating rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain LOL:ParticipantRole ;
                        rdfs:range LOL:participation:Object .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#part_of_class
LOL:part_of_class rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf LOL:isPartOf ;
                  rdfs:domain LOL:Campione ;
                  rdfs:range LOL:Classe ;
                  rdfs:comment "Represents the classes a Champions belongs to"@en ;
                  rdfs:label "part of class"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#participatingInEvent
LOL:participatingInEvent rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain LOL:ParticipantRole ;
                         rdfs:range LOL:participation:Event ;
                         rdfs:comment "Relazione che indica la partecipazione a un evento da parte di un player"@it ;
                         rdfs:label "participatingInEvent"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#participatingInMatch
LOL:participatingInMatch rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf LOL:participatingInEvent ;
                         rdfs:domain LOL:Match_role ;
                         rdfs:range LOL:Match ;
                         rdfs:comment "Relazione che indica la partecipazione a un match da parte di un player"@it ;
                         rdfs:label "participatingInMatch"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#picked_champion
LOL:picked_champion rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain LOL:Loadout ;
                    rdfs:range LOL:Campione ;
                    rdfs:comment "Relazione che indica la scelta di un campione da parte di un player"@it ;
                    rdfs:label "picked_champion"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#picked_in_loadout
LOL:picked_in_loadout rdf:type owl:ObjectProperty ,
                               owl:InverseFunctionalProperty ;
                      rdfs:domain LOL:Rune_Page ;
                      rdfs:range LOL:Loadout ;
                      rdfs:comment "Relazione che indica il Loadout in cui è selezionata una rune page"@it ;
                      rdfs:label "picked_in_loadout"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#picked_rune_page
LOL:picked_rune_page rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain LOL:Loadout ;
                     rdfs:range LOL:Rune_Page ;
                     rdfs:comment "Relazione che indica la scelta di un rune page in un Loadout"@it ;
                     rdfs:label "picked_rune_page"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#picked_summoner_spells
LOL:picked_summoner_spells rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf owl:topObjectProperty ;
                           rdfs:domain LOL:Loadout ;
                           rdfs:range LOL:Summoner_Spells ;
                           rdfs:comment "Relazione che indica le summoner spells scelte in un Loadout"@it ;
                           rdfs:label "picked_summoner_spells"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#playedInRole
LOL:playedInRole rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:comment "States that a champion is played in a certain role"@en ;
                 rdfs:label "playedInRole"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#played_in_map
LOL:played_in_map rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain LOL:Modalità ;
                  rdfs:range LOL:Mappa ;
                  rdfs:comment "Relazione che indica la modalità scelta relativa alla mappa"@it ;
                  rdfs:label "played_in_map"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#played_on_map
LOL:played_on_map rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain LOL:Match ;
                  rdfs:range LOL:Mappa ;
                  rdfs:comment "Relazione che indica il match giocato su quella mappa"@it ;
                  rdfs:label "played_on_map"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#played_with_modality
LOL:played_with_modality rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain LOL:Match ;
                         rdfs:range LOL:Modalità ;
                         rdfs:comment "Relazione che indica la mappa giocata relativa alla modalità"@it ;
                         rdfs:label "played_with_modality"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#playerParticipating
LOL:playerParticipating rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf LOL:objectParticipating ;
                        rdfs:domain LOL:Match_role ;
                        rdfs:range LOL:Player ;
                        rdfs:comment "Relazione che indica i player che partecipano al match"@it ;
                        rdfs:label "playerParticipating"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#positionOfParticipant
LOL:positionOfParticipant rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf LOL:roleOfParticipant ;
                          rdfs:domain LOL:Match_role ;
                          rdfs:range LOL:Ruoli ;
                          rdfs:comment "Relazione che indica il posizione in cui i player partecipano al match"@it ;
                          rdfs:label "positionOfParticipant"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#roleOfParticipant
LOL:roleOfParticipant rdf:type owl:ObjectProperty ;
                      rdfs:domain LOL:ParticipantRole ;
                      rdfs:range LOL:Role ;
                      rdfs:comment "Relazione che indica il ruolo in cui i player partecipano al match"@it ;
                      rdfs:label "roleOfParticipant"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#same_class
LOL:same_class rdf:type owl:ObjectProperty ,
                        owl:SymmetricProperty ,
                        owl:TransitiveProperty .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#scales_with_stat
LOL:scales_with_stat rdf:type owl:ObjectProperty ;
                     rdfs:domain LOL:Abilità ,
                                 LOL:Campione ;
                     rdfs:range LOL:Statistiche ;
                     owl:propertyChainAxiom ( LOL:has_ability
                                              LOL:scales_with_stat
                                            ) ;
                     rdfs:comment "Relazione che indica la possibilità di scalare con le statistiche"@it ;
                     rdfs:label "scales_with_stat"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#participation:hasParticipant
LOL:participation:hasParticipant rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#participation:isParticipatingIn
LOL:participation:isParticipatingIn rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/gfazor/ontologies/LOL#AbilityPower
LOL:AbilityPower rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf LOL:Statistics ;
                 rdfs:comment "Ability Power Value"@en ;
                 rdfs:label "AbilityPower"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#ArmorValue
LOL:ArmorValue rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf LOL:Statistics ;
               rdfs:comment "Statistic Armor Value"@en ;
               rdfs:label "Armor"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#AttackDamage
LOL:AttackDamage rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf LOL:Statistics ;
                 rdfs:comment "Attack Damage Value"@en ;
                 rdfs:label "AttackDamage"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#AttackSpeed
LOL:AttackSpeed rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf LOL:Statistics ;
                rdfs:comment "Attack Speed Value"@en ;
                rdfs:label "AttackSpeed"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#CritChance
LOL:CritChance rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf LOL:Statistics ;
               rdfs:comment "Critical Strike Chance Value"@en ;
               rdfs:label "CritChance"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#HitPoints
LOL:HitPoints rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf LOL:Statistics ;
              rdfs:comment "Hit Points Value"@en ;
              rdfs:label "HitPoints"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Lethality
LOL:Lethality rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf LOL:Statistics .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#MagicResist
LOL:MagicResist rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf LOL:Statistics ;
                rdfs:comment "Magic Resistance value"@en ;
                rdfs:label "MagicResist"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#MovementSpeed
LOL:MovementSpeed rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf LOL:Statistics ;
                  rdfs:comment "Movement Speed Value"@en ;
                  rdfs:label "MovementSpeed"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Statistics
LOL:Statistics rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain LOL:Entity ;
               rdfs:range xsd:float ;
               rdfs:comment "Entities Statistics values"@en ;
               rdfs:label "Statistics"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#ability_description
LOL:ability_description rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf owl:topDataProperty ;
                        rdfs:domain LOL:Abilità ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Descrizione dell'abilità"@it ;
                        rdfs:label "ability_description"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#has_price
LOL:has_price rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain LOL:Oggetto ;
              rdfs:range xsd:int ;
              rdfs:comment "Prezzo dell'oggetto"@it ;
              rdfs:label "has_price"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#lore
LOL:lore rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain LOL:Campione ;
         rdfs:range xsd:string ;
         rdfs:comment "Storia del campione"@it ;
         rdfs:label "lore"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#rank
LOL:rank rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain LOL:Player ;
         rdfs:range xsd:string ;
         rdfs:comment "rank del giocatore"@it ;
         rdfs:label "rank"@it .


#################################################################
#    Classes
#################################################################

###  http://data.totl.net/game.rdf#Player
totl:Player rdf:type owl:Class ;
            owl:equivalentClass wd:Q5276395 ;
            rdfs:subClassOf LOL:participation:Object .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Abilità
LOL:Abilità rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty LOL:scales_with_stat ;
                                  owl:someValuesFrom LOL:Statistiche
                                ] ;
            rdfs:subClassOf LOL:League_Of_Legends ;
            rdfs:comment "Rappresenta le abilità utilizzabili da un campione"@it ;
            rdfs:label "Abilità"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Assassin
LOL:Assassin rdf:type owl:Class ;
             rdfs:subClassOf LOL:Campione ,
                             LOL:Classe ;
             rdfs:comment "Represents champions with class Assassin"@en ;
             rdfs:label "Assassin"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Attack
LOL:Attack rdf:type owl:Class ;
           owl:equivalentClass dot:Damage ,
                               [ rdf:type owl:Class ;
                                 owl:oneOf ( LOL:Attack_Damage
                                             LOL:Attack_Speed
                                             LOL:Crit_chance
                                             LOL:Lethality
                                           )
                               ] ;
           rdfs:subClassOf LOL:Statistiche ;
           rdfs:comment "Represents stats that are correlated to attack damage"@en ;
           rdfs:label "Attack"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Bruiser
LOL:Bruiser rdf:type owl:Class ;
            owl:equivalentClass wd:Q1250916 ;
            rdfs:subClassOf LOL:Campione ,
                            LOL:Classe ;
            rdfs:comment "Represents champions with class Bruiser"@en ;
            rdfs:label "Bruiser"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Campione
LOL:Campione rdf:type owl:Class ;
             owl:equivalentClass wd:Q1062345 ,
                                 [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty LOL:has_ability ;
                                                          owl:someValuesFrom LOL:Passiva
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty LOL:has_ability ;
                                                          owl:minQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                                          owl:onClass LOL:Abilità
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty LOL:has_ability ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass LOL:Ultimate
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty LOL:part_of_class ;
                                   owl:someValuesFrom LOL:Classe
                                 ] ;
             rdfs:subClassOf LOL:Entity ;
             rdfs:comment "Rappresenta entità di tipo campione"@it ;
             rdfs:label "Campione"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Classe
LOL:Classe rdf:type owl:Class ;
           rdfs:subClassOf LOL:League_Of_Legends ;
           rdfs:comment "Rappresenta le classi di un campione"@it ;
           rdfs:label "Classe"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Defense
LOL:Defense rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( LOL:Armor
                                              LOL:HP
                                              LOL:HP_Regen
                                              LOL:Magic_Resist
                                            )
                                ] ;
            rdfs:subClassOf LOL:Statistiche ;
            rdfs:comment "Represents defensive statistics"@en ;
            rdfs:label "Defense"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Entity
LOL:Entity rdf:type owl:Class ;
           rdfs:subClassOf LOL:League_Of_Legends ;
           rdfs:comment "Represents entities in game"@en ;
           rdfs:label "Entity"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Keystone
LOL:Keystone rdf:type owl:Class ;
             rdfs:subClassOf LOL:Path_Rune ;
             owl:disjointWith LOL:Other_Rune ;
             rdfs:comment "Represents a rune of type keystone"@en ;
             rdfs:label "Keystone"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#League_Of_Legends
LOL:League_Of_Legends rdf:type owl:Class ;
                      owl:equivalentClass wd:Q223341 ;
                      rdfs:comment "Rappresenta il gioco League of Legends"@it ;
                      rdfs:label "League of Legends"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Loadout
LOL:Loadout rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                         owl:onProperty LOL:picked_champion ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass LOL:Campione
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty LOL:picked_rune_page ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass LOL:Rune_Page
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty LOL:picked_summoner_spells ;
                                                         owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                         owl:onClass LOL:Summoner_Spells
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty LOL:bought_objects ;
                                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass LOL:Stivale
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty LOL:bought_objects ;
                                                         owl:maxQualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                                         owl:onClass LOL:Oggetto
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf LOL:Role ;
            rdfs:comment "Represents the pick, the runes and the items choosen by a player in a game"@en ;
            rdfs:label "Loadout"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Mage
LOL:Mage rdf:type owl:Class ;
         rdfs:subClassOf LOL:Campione ,
                         LOL:Classe ;
         rdfs:comment "Represents champions with class Mage"@en ;
         rdfs:label "Mage"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Magic
LOL:Magic rdf:type owl:Class ;
          owl:equivalentClass spin:MagicProperty ,
                              [ rdf:type owl:Class ;
                                owl:oneOf ( LOL:Ability_Haste
                                            LOL:Ability_Power
                                            LOL:Magic_Pen
                                          )
                              ] ;
          rdfs:subClassOf LOL:Statistiche ;
          rdfs:comment "Represents statistics correlated with magic damage"@en ;
          rdfs:label "Magic"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Mappa
LOL:Mappa rdf:type owl:Class ;
          rdfs:subClassOf LOL:League_Of_Legends ;
          rdfs:comment "Rappresenta le mappe presenti nel gioco"@it ;
          rdfs:label "Mappa"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Marksman
LOL:Marksman rdf:type owl:Class ;
             rdfs:subClassOf LOL:Campione ,
                             LOL:Classe ;
             rdfs:comment "Represents champions with class Marksman"@en ;
             rdfs:label "Marksman"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Match
LOL:Match rdf:type owl:Class ;
          owl:equivalentClass so:Match ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty LOL:has_modality ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass LOL:Modalità
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty LOL:played_on_map ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass LOL:Mappa
                              ] ;
          rdfs:subClassOf LOL:participation:Event ;
          rdfs:comment "Represents a game match played"@en ;
          rdfs:label "Match"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Match_role
LOL:Match_role rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty LOL:loadoutOfParticipant ;
                                                            owl:someValuesFrom LOL:Loadout
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty LOL:participatingInMatch ;
                                                            owl:someValuesFrom LOL:Match
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty LOL:playerParticipating ;
                                                            owl:someValuesFrom LOL:Player
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty LOL:positionOfParticipant ;
                                                            owl:someValuesFrom LOL:Ruoli
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf LOL:ParticipantRole ;
               rdfs:comment "Represents the association between a match, the players that play the match, the roles of the players and their Loadouts"@en ;
               rdfs:label "Match_role"@en .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Modalità
LOL:Modalità rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty LOL:played_in_map ;
                                   owl:someValuesFrom LOL:Mappa
                                 ] ;
             rdfs:subClassOf LOL:League_Of_Legends ;
             rdfs:comment "Rappresenta le modalità di gioco presenti"@it ;
             rdfs:label "Modalità"@it .


###  http://www.semanticweb.org/gfazor/ontologies/LOL#Modalità_a_rotazione
LOL:Modalità_a_rotazione rdf:type owl:Class ;
                         rdfs:subClassOf LOL:Modalità ;
                         rdfs:comment "Rappresenta le modalità di gioco non permanenti"@it ;
                         rdfs:label "Modalità_a_rotazione"@it .


