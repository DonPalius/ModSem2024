#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( LOL:Campione
                LOL:NPC
                LOL:Oggetto
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( LOL:Domination
                        LOL:Inspiration
                        LOL:Precision
                        LOL:Resolve
                        LOL:Sorcery
                      )
] .


[ rdf:type owl:AllDisjointProperties ;
  owl:members ( LOL:AbilityPower
                LOL:ArmorValue
                LOL:AttackDamage
                LOL:AttackSpeed
                LOL:CritChance
                LOL:HitPoints
                LOL:Lethality
                LOL:MagicResist
              )
] .


