#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( LoL:Campione
                LoL:NPC
                LoL:Oggetto
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( LoL:Oggetto_Rimosso
                LoL:Stivale
                LOL:Oggetto_Leggendario
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( LoL:Domination
                        LoL:Inspiration
                        LoL:Precision
                        LoL:Resolve
                        LoL:Sorcery
                      )
] .


[ rdf:type owl:AllDisjointProperties ;
  owl:members ( LoL:ability_description
                LoL:has_price
                LoL:lore
                LoL:match_date
                LoL:number_player
                LoL:rank
                LoL:release_date
                LOL:Statistics
              )
] .


[ rdf:type owl:AllDisjointProperties ;
  owl:members ( LOL:AbilityPower
                LOL:ArmorValue
                LOL:AttackDamage
                LOL:AttackSpeed
                LOL:CritChance
                LOL:HitPoints
                LOL:Lethality
                LOL:MagicResist
              )
] .


#################################################################
#    Rules
#################################################################

:x rdf:type swrl:Variable .

:y rdf:type swrl:Variable .

:z rdf:type swrl:Variable .

:k rdf:type swrl:Variable .

:h rdf:type swrl:Variable .

:e rdf:type swrl:Variable .

:v rdf:type swrl:Variable .

:o rdf:type swrl:Variable .

<urn:swrl:var#o> rdf:type swrl:Variable .

<urn:swrl:var#v> rdf:type swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Asserisce che se un giocatore ha giocato un campione in un ruolo, allora il campione è giocato in quel ruolo" ;
   rdfs:label "giocatoInRuolo" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate LoL:Match_role ;
                           swrl:argument1 :x
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:ClassAtom ;
                                      swrl:classPredicate LoL:Player ;
                                      swrl:argument1 :y
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:ClassAtom ;
                                                 swrl:classPredicate LoL:Loadout ;
                                                 swrl:argument1 :z
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:ClassAtom ;
                                                            swrl:classPredicate LoL:Ruoli ;
                                                            swrl:argument1 :k
                                                          ] ;
                                                rdf:rest [ rdf:type swrl:AtomList ;
                                                           rdf:first [ rdf:type swrl:ClassAtom ;
                                                                       swrl:classPredicate LoL:Campione ;
                                                                       swrl:argument1 :h
                                                                     ] ;
                                                           rdf:rest [ rdf:type swrl:AtomList ;
                                                                      rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                                  swrl:propertyPredicate LoL:playerParticipating ;
                                                                                  swrl:argument1 :x ;
                                                                                  swrl:argument2 :y
                                                                                ] ;
                                                                      rdf:rest [ rdf:type swrl:AtomList ;
                                                                                 rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                                             swrl:propertyPredicate LoL:positionOfParticipant ;
                                                                                             swrl:argument1 :x ;
                                                                                             swrl:argument2 :k
                                                                                           ] ;
                                                                                 rdf:rest [ rdf:type swrl:AtomList ;
                                                                                            rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                                                        swrl:propertyPredicate LoL:loadoutOfParticipant ;
                                                                                                        swrl:argument1 :x ;
                                                                                                        swrl:argument2 :z
                                                                                                      ] ;
                                                                                            rdf:rest [ rdf:type swrl:AtomList ;
                                                                                                       rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                                                                                   swrl:propertyPredicate LoL:picked_champion ;
                                                                                                                   swrl:argument1 :z ;
                                                                                                                   swrl:argument2 :h
                                                                                                                 ] ;
                                                                                                       rdf:rest rdf:nil
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate LOL:playedInRole ;
                           swrl:argument1 :h ;
                           swrl:argument2 :k
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If an item has AttackDamage stat then the Item has an object property 'has_stat' with 'AttackDamage'" ;
   rdfs:label "Has Attack Damage" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate LoL:Oggetto ;
                           swrl:argument1 :e
