###  http://www.semanticweb.org/gfazor/ontologies/LOL#Rune_Page
LOL:Rune_Page rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                           owl:onProperty LOL:has_keystone ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass LOL:Keystone
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty LOL:has_primary_path ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass LOL:Path
                                                         ]
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty LOL:has_secondary_path ;
                                                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass LOL:Path
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty LOL:has_rune ;
                                    owl:qualifiedCardinality "6"^^xsd:nonNegativeInteger ;
                                    owl:onClass LOL:Path_Rune
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty LOL:has_shard ;
                                    owl:qualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                    owl:onClass LOL:Shard
                                  ] ;
              rdfs:subClassOf LOL:League_Of_Legends ;
              rdfs:comment "Insieme di rune scelte dal giocatore"@it ;
              rdfs:label "Rune_Page"@it .


